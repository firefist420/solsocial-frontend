{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/feed",
      "dest": "/feed"
    },
    {
      "src": "/token/(.*)",
      "dest": "/token/[address]"
    }
  ],
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "env": {
    "NEXT_PUBLIC_HCAPTCHA_SITEKEY": "@hcaptcha_sitekey"
  }
}